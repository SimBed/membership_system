  <section class="py-15 py-xl-20 bg-black inverted">
    <div class="container">
      <div class="row">
        <div class="col-lg-8">
          <div class="accordion accordion-minimal accordion-highlight" id="accordion-1">
            <div class="accordion-item">
              <h2 class="accordion-header" id="heading-1-1">
