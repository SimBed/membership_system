<%# still used?%>
<h4>Restart</h4>
<%= turbo_frame_tag 'restart' do %>
  <div class="canvas gray100-canvas embossed width32 text-center mb-3">
    <div class="px-4">
      <p>Started your Package, but not attended as many classes as you planned? Please read about our Restart option under 'Modifications' <%= link_to 'here', terms_and_conditions_path, class: "fw-bolder text-decoration-underline", target: '_blank' %></p>  
    </div>
    <div class="cancel">
      <%= link_to image_tag('delete.png', class: "table_icon"), client_package_modification_cancel_restart_path(@client) %>
    </div>
  </div>
<% end %>