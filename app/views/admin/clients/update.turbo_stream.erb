<%#= turbo_stream.prepend "flash", partial: "layouts/flashhot" %>
<%= turbo_stream.update "insta-#{@client.id}" do %>
  <%= render partial: "insta", locals: { client: @client } %>
<% end %>
<%= turbo_stream.update "waiver-#{@client.id}" do %>
  <%= render partial: "waiver", locals: { client: @client } %>
<% end %>
<%= turbo_stream.update "whatsapp-group-#{@client.id}" do %>
  <%= render partial: "whatsapp_group", locals: { client: @client } %>
<% end %>
<turbo-stream action="prepend" target="flash">
  <template>
    <%= render_flash %> 
  </template>
</turbo-stream>