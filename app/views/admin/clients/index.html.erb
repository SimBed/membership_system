<div class="container px-1">
  <h4>Clients <%= link_to image_tag('add.png', class: "button_icon"),
                          new_client_path,
                          data: { turbo_frame: dom_id(Client.new) } %>
  </h4>
  <%= turbo_frame_tag Client.new %>    
  <!--h3 style="">Clients <%#= link_to tag.i(class: %w( bi bi-plus-circle )), new_admclient_path, class: "icon-container"%></h3-->
</div>
<%= turbo_frame_tag 'clients-and-filter' do %>
  <%= render partial: 'filter_form' %>
  <%#= turbo_frame_tag 'client_edit' %>    
  <% if logged_in_as?('superadmin') %>
    <div class="container px-1 mb-2">  
      <%= render partial: 'download' %>
    </div>
  <% end %>
  <%= turbo_frame_tag 'clients' do %>
    <%= render partial: 'clients_table' %>
  <% end %>
<% end %>
