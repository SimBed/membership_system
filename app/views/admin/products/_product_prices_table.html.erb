<div id="product-prices-table" class="container px-1 mb-5">
  <table class="table table-secondary table-striped caption-top">
    <!--caption>Product Categories</caption-->
    <tbody>
      <tr>
        <th scope="col">Base Price</th>
        <th scope="col">From</th>
        <th scope="col">Until</th>
        <th scope="col">Count</th>
        <th colspan="3"><%= link_to image_tag('add.png', class:'table_icon'), new_admin_price_path(product_id: params[:id]), data: {turbo: false} %></th>
      </tr>
      <%= render partial: 'admin/products/product_price', collection: @prices, as: :price %>
    </tbody>
  </table>
</div>
