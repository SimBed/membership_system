<p id="notice"><%= notice %></p>

<h1>Timetables</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetables.each do |timetable| %>
      <tr>
        <td><%= timetable.title %></td>
        <td><%= link_to 'Show', [:admin, timetable] %></td>
        <td><%= link_to 'Edit', edit_admin_timetable_path(timetable) %></td>
        <td><%= link_to 'Destroy', [:admin, timetable], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timetable', new_admin_timetable_path %>
