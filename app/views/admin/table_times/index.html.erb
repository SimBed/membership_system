<p id="notice"><%= notice %></p>

<h1>Table Times</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @table_times.each do |table_time| %>
      <tr>
        <td><%= table_time.start %></td>
        <td><%= link_to 'Show', [:admin, table_time] %></td>
        <td><%= link_to 'Edit', edit_admin_table_time_path(table_time) %></td>
        <td><%= link_to 'Destroy', [:admin, table_time], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Table Time', new_admin_table_time_path %>
