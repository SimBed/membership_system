<div class="text-center">
  <div class="">
    <%= link_to 'strength', shared_strength_markers_path, class:'option_choice me-1' %>
    <%= link_to 'body', shared_body_markers_path, class:'option_choice current' %>
  </div>
</div>

<div class="container px-1">
  <h4>Body Markers <%= link_to image_tag('add.png', class: "button_icon"), new_shared_body_marker_path %></h4>
</div>
<%= turbo_frame_tag 'body_markers' do %>
  <%= render partial: 'filter_form' %>
  <%= render partial: 'body_marker_chart' unless session[:client_select] == 'All' %>
  <%= render 'body_markers_table' %>
<% end %>
