<%= turbo_stream.update "body-markers-table" do %>
  <%= render partial: "body_markers_table" %>
<% end %>
<% unless session[:client_select] == 'All' %>
  <%= turbo_stream.update "body-markers-chart" do %>
    <%= render partial: 'body_markers_chart' unless session[:client_select] == 'All' %>
  <% end %>
<% end %>
<%= turbo_stream.prepend "flash", partial: "partials/flashes" %>
<%= turbo_stream.remove "errors" %>