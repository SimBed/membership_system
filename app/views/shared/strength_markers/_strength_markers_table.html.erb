    <div class="row fw-bolder">
      <div class="column col-1x">Date</div>
      <div class="column col-1x">Activity</div>
      <div class="column col-1x">Weight</div>
      <div class="column col-1x">Reps</div>
      <div class="column col-1x">Sets</div>
