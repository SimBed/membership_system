   <%= javascript_pack_tag 'headroom' %>
   <nav id="mainNav" class="headroom navbar navbar-expand-lg navbar-sticky navbar-dark bg-black inverted">

      <div class="container">
        <%= link_to root_path, class: "navbar-brand" do %>
          <%= image_tag('SpaceLogo_white.png', alt: "logo") %>
        <% end %>

        <!-- secondary -->
        <ul class="navbar-nav navbar-nav-secondary order-lg-3">
          <li class="nav-item d-lg-none">
            <a class="nav-link nav-icon" href="" role="button" data-bs-toggle="collapse" data-bs-target="#navbar"
              aria-controls="navbar" aria-expanded="false" aria-label="Toggle navigation">
              <span class="bi bi-list"></span>
            </a>
          </li>
        </ul>

        <!-- primary -->
        <div class="collapse navbar-collapse" id="navbar" data-bs-parent="#mainNav">
          <ul class="navbar-nav">
            <li class="nav-item">
              <%= link_to admin_clients_path, class: "nav-link" do %>
                <i class="fa fa-address-card"></i>
              <% end %>
            </li>
            <li class="nav-item">
              <%= link_to admin_purchases_path, class: "nav-link" do %>
                <i class="fa fa-cc-visa"></i>
              <% end %>
            </li>
            <li class="nav-item">
              <%= link_to admin_wkclasses_path, class: "nav-link" do %>
                <i class="fa fa-dumbbell"></i>
              <% end %>
            </li>
            <li class="nav-item">
              <%= link_to public_timetable_path, class: "nav-link" do %>
                <i class="fa fa-calendar"></i>
              <% end %>
            </li>
          </ul>

          <ul class="navbar-nav">

            <li class="nav-item dropdown dropdown-hover">
              <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown-1" role="button" data-bs-toggle="dropdown"
                aria-expanded="false">
                <i class="fa fa-list"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-custom" aria-labelledby="navbarDropdown-1">
                <div class="row g-0">
                  <div class="col-6 p-4">
                    <% unless logged_in_as?('junioradmin') %>    
                      <span class="dropdown-label">JUNIORADMIN</span>
                    <% end %>  
                      <%#= link_to 'Fitternity', admin_fitternities_path, class: "dropdown-item" %>
                      <%= link_to 'Products', admin_products_path, class: "dropdown-item" %>
                    <% unless logged_in_as?('junioradmin') %>                     
                      <span class="dropdown-label">ADMIN</span>
                      <%= link_to 'Instructors', admin_instructors_path, class: "dropdown-item" %>
                      <%= link_to 'Partners', admin_partners_path, class: "dropdown-item" %>
                      <%= link_to 'Timetables', admin_timetables_path, class: "dropdown-item" %>                           
                      <%= link_to 'Workouts', admin_workouts_path, class: "dropdown-item" %>
                      <%= link_to 'Workout Groups', admin_workout_groups_path, class: "dropdown-item" %>
                    <% end %>  
                      <% if logged_in_as?('superadmin') %>                   
                        <span class="dropdown-label">SUPERADMIN</span>
                        <%= link_to 'Accounts', admin_accounts_path, class: "dropdown-item" %>
                        <%= link_to 'Expenses', superadmin_expenses_path, class: "dropdown-item" %>
                        <%= link_to 'Regular Expenses', superadmin_regular_expenses_path, class: "dropdown-item" %>
                        <%= link_to 'Instructor rates', superadmin_instructor_rates_path, class: "dropdown-item" %>
                        <%= link_to 'Razorpay Orders', superadmin_orders_path, class: "dropdown-item" %>
                        <%= link_to 'Settings', superadmin_settings_path, class: "dropdown-item" %>
                      <% end %>                                                                      
                  </div>
                  <% if multiple_roles? %>                  
                    <div class="col-6 p-4">
                      <span class="dropdown-label">ROLE</span>
                      <%= content_tag :div, "*#{current_role}", class: "dropdown-item text-uppercase" %>                
                      <% navbar_roles.each do |role| %>
                        <%= link_to role.name, "/switch_account_role?role=#{role.name}", class: "dropdown-item" %>
                      <% end %>
                    </div>  
                  <% end %>                
                </div>
              </div>
            </li>

            <li class="nav-item">
              <% if current_account %>
                <%= link_to logout_path, method: :delete, class: "nav-link active" do %>
                  <i class="fa fa-door-open"></i>
                <% end %>
              <% else %>
                <%= link_to login_path, class: "nav-link" do %>
                  <i class="fa fa-sign-in-alt"></i>
                  <span class="link-text">Login</span>
                <% end %>
              <% end %>
            </li>

          </ul>
        </div>
      </div>
    </nav>